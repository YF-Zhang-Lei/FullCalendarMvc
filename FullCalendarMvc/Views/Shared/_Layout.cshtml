<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="@Url.Content("~/Content/FullCalendar/fullcalendar.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" />

    <script src="@Url.Content("~/Scripts/FullCalendar/moment.min.js")" ></script>
    @Scripts.Render("~/bundles/jquery")
    <script src="@Url.Content("~/Scripts/FullCalendar/fullcalendar.min.js")" ></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")" ></script>
    <script src="@Url.Content("~/Scripts/jquery-ui.custom.min.js")" ></script>
</head>
<body>
    @RenderBody()

    @RenderSection("scripts", required: false)
</body>
</html>
